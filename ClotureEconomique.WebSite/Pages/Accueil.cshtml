@page
@{
    ViewData["Title"] = "Accueil";
}

<div class="banner-carousel banner-carousel-1 mb-0">
    <div class="banner-carousel-item" style="background-image: url(images/projects/cloture_maille_latte_ornemental.jpg)">
        <div class="slider-content">
            <div class="container h-100">
                <div class="row align-items-center h-100">
                    <div class="col-md-12 text-center">
                        <h2 class="slide-title" data-animation-in="slideInLeft">Services de clôtures</h2>
                        @*<h3 class="slide-sub-title" data-animation-in="slideInRight">Services de clôture</h3>*@
                        <p data-animation-in="slideInLeft" data-duration-in="1.2">
                            <a href="/Clotures" class="slider btn btn-primary">Nos clôtures</a>
                            <a href="/Contact" class="slider btn btn-primary border">Contactez nous</a>
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="banner-carousel-item" style="background-image: url(images/projects/deneigement-toit-entreprise.webp)">
        <div class="slider-content text-left">
            <div class="container h-100">
                <div class="row align-items-center h-100">
                    <div class="col-md-12">
                        @*<h2 class="slide-title-box" data-animation-in="slideInDown">World Class Service</h2>*@
                        <h3 class="slide-title" data-animation-in="fadeIn">Déneigement</h3>
                        @*<h3 class="slide-sub-title" data-animation-in="slideInLeft">Your Choice is Simple</h3>*@
                        <p data-animation-in="slideInRight">
                            <a href="/Deneigement" class="slider btn btn-primary border">Nos services</a>
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="banner-carousel-item" style="background-image:url(images/slider-main/bg3.jpg)">
        <div class="slider-content text-right">
            <div class="container h-100">
                <div class="row align-items-center h-100">
                    <div class="col-md-12">
                        @*<h2 class="slide-title" data-animation-in="slideInDown">Tonte de pelouse</h2>*@
                        <h3 class="slide-title" data-animation-in="fadeIn">Tonte de pelouse</h3>
                        @*<p class="slider-description lead" data-animation-in="slideInRight">We will deal with your failure that determines how you achieve success.</p>*@
                    <div data-animation-in="slideInLeft">
                        <a href="/Tonte_pelouse" class="slider btn btn-primary border" aria-label="learn-more-about-us">Nos services</a>
                        <a href="/Soumission" class="slider btn btn-primary" aria-label="contact-with-us">Obtenez une soumission gratuite</a>
                    </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<section class="call-to-action-box no-padding">
    <div class="container">
        <div class="action-style-box">
            <div class="row align-items-center">
                <div class="col-md-8 text-center text-md-left">
                    <div class="call-to-action-text">
                        <h3 class="action-title">Nous comprenons vos besoins en clôture</h3>
                    </div>
                </div><!-- Col end -->
                <div class="col-md-4 text-center text-md-right mt-3 mt-md-0">
                    <div class="call-to-action-btn">
                        <a class="btn btn-dark" href="/Soumission">Obtenir une soumission</a>
                    </div>
                </div><!-- col end -->
            </div><!-- row end -->
        </div><!-- Action style box -->
    </div><!-- Container end -->
</section><!-- Action end -->

